                </place>
                <place x="16.6cm" y="20.8cm" width="5cm" height="0.5cm">
                    <para style="terp_default_7">[[ picking.move_lines and picking.move_lines[0].sale_line_id.configurator_id.masse_plis ]]</para>
                </place>
                <place x="16.6cm" y="20.6cm" width="5cm" height="0.3cm">
                    <para style="terp_default_7">[[ picking.move_lines and picking.move_lines[0].sale_line_id.configurator_id.masse_plis ]]</para>
                </place>
                <place x="16.6cm" y="20.3cm" width="5cm" height="0.3cm">
                    <para style="terp_default_7">[[ picking.move_lines and picking.move_lines[0].sale_line_id.price_unit ]]</para>
                </place>
                <place x="16.6cm" y="19.9cm" width="5cm" height="0.3cm">
                    <para style="terp_default_7">[[ picking.move_lines and picking.move_lines[0].product_qty ]]</para>
                </place>
                <place x="16.6cm" y="19.6cm" width="5cm" height="0.3cm">
