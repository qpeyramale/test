                </place>
                
                <place x="11.1cm" y="22.34cm" width="1cm" height="0.3cm">
                    <para style="terp_default_7">[[ picking.move_lines and picking.move_lines[0].product_qty>=100 and 'x' or removeParentNode('para') ]]</para>
                </place>
                <place x="11.1cm" y="21.96cm" width="1cm" height="0.3cm">
                    <para style="terp_default_7">[[ picking.move_lines and picking.move_lines[0].product_qty>=800 and 'x' or removeParentNode('para') ]]</para>
                </place>
